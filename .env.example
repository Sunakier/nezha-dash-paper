# Nezha Dashboard Paper Customization Environment Variables

# Custom Logo URL (default: "/apple-touch-icon.png")
# VITE_CUSTOM_LOGO="/path/to/your/logo.png"

# Custom Description (default: translation key "nezha")
# VITE_CUSTOM_DESC="Your Custom Description"

# Custom Links (JSON string format)
# VITE_CUSTOM_LINKS='[{"name":"GitHub","link":"https://github.com"},{"name":"Docs","link":"https://docs.example.com"}]'

# Custom Background Image URL
# VITE_CUSTOM_BACKGROUND_IMAGE="https://example.com/background.jpg"

# Custom Mobile Background Image URL
# VITE_CUSTOM_MOBILE_BACKGROUND_IMAGE="https://example.com/mobile-background.jpg"

# Custom Illustration Image URL (default: "/animated-man.webp")
# VITE_CUSTOM_ILLUSTRATION="/path/to/your/illustration.png"

# Force Theme (light or dark)
# VITE_FORCE_THEME="dark"

# Force Use SVG Flag (true or false)
# VITE_FORCE_USE_SVG_FLAG="true"

# Force Show Services (true or false)
# VITE_FORCE_SHOW_SERVICES="true"

# Force Card Inline (true or false)
# VITE_FORCE_CARD_INLINE="true"

# Force Show Map (true or false)
# VITE_FORCE_SHOW_MAP="true"

# Force Peak Cut Enabled (true or false)
# VITE_FORCE_PEAK_CUT_ENABLED="true"

# Disable Animated Man (true or false)
# VITE_DISABLE_ANIMATED_MAN="true"

# Proxy Configuration
# WebSocket proxy target (default: "ws://127.0.0.1:8008")
# VITE_PROXY_WS_TARGET="ws://your-server-address:port"
# HTTP proxy target (default: "http://127.0.0.1:8008")
# VITE_PROXY_HTTP_TARGET="http://your-server-address:port"
